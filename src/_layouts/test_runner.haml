!!!
%html
  %head
    %title TestRunner
    %script{:src => "../javascripts/vendor/underscore-min.js", :type => "text/javascript"}  
    %script{:src => "../javascripts/vendor/jquery-1.6.2.min.js", :type => "text/javascript"}
    %script{:src => "../javascripts/vendor/jquery.scrollto.min.js", :type => "text/javascript"}

    %link{:href => "../spec/lib/jasmine-1.1.0.rc1/jasmine.css", :rel => "stylesheet", :type => "text/css"}
    :css
      .runner {
        position: fixed;
        top: 0;
        left: 0;
      }
      .passed,
      .skipped {
        display: block;
      }
    
  %body
    = yield